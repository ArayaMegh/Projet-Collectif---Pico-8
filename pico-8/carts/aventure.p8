--interface

function draw_ui()
	palt(0,false)
	palt(3,true)
	spr(3,2,2)
	palt()
	print_outline("X"..p.item, 10,2,7)
end

function print_outline(text,x,y)
	print(text,x-1,y,0)
	print(text,x+1,y,0)
	print(text,x,y-1,0)
	print(text,x,y+1,0)
	print(text,x,y,7)
end
