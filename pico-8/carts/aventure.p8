function draw_dialog_box(x, y, name, message)
	if name then
	 rectfill(x + 2, y - 8, x + #name * 4 + 6, y-1, 2)
		print(name, x + 5, y - 6, 15)
	end 
	rectfill(x, y, x+124, y+20, 2)
	rectfill(x + 1, y + 1, x+123, y+19, 4)
 print(message, x+3, y+3, 0)
end
