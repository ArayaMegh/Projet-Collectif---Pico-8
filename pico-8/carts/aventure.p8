--map
function draw_map()
	map(0,0,0,0,128,64)	
end

function check_flag(flag,x,y)
	local sprite=mget(x,y)
	return	fget(sprite,flag)
end

function update_camera()
	local camx=flr(p.x/16)*16
	local camy=flr(p.y/16)*16
	camera(camx*8,camy*8)
end

function next_item(x,y)
	local sprite=mget(x,y)
	mset(x,y,sprite+1)
end

function open_bridge(x,y)
	next_item(x,y)
end

function open_labyrinthe(x,y)
	local paille=mget(x,y)
	mset(x,y,42)
end
-->8
--player

function create_player()
	p={
		x=5,y=2,
		ox=0,oy=0,
		start_ox=0,start_oy=0,
		anim_t=0,
		sprite=17,
		keys=0,
		item=0
	}
end

function player_movement()
	newx=p.x
	newy=p.y
	if p.anim_t == 0 then
  newox = 0
  newoy = 0
  if btn(⬅️) then
   newx -= 1
   newox = 8
   p.flip=true
  elseif btn(➡️) then
   newx += 1
   newox = -8
   p.flip=false
  elseif btn(⬆️) then
   newy -= 1
   newoy = 8
  elseif btn(⬇️) then
   newy += 1
   newoy = -8
  elseif btnp(❎	) then
  	attack()
  end
 end
	
	if (newx!=p.x or newy!=p.y) and
	not check_flag(0,newx,newy) then
		p.x=mid(0,newx,127)
		p.y=mid(0,newy,63)
		p.start_ox=newox
		p.start_oy=newoy
		p.anim_t=1
	end
		
	check_item(p.x,p.y)
	p.anim_t=max(p.anim_t-0.125,0)
	p.ox=p.start_ox*p.anim_t
	p.oy=p.start_oy*p.anim_t
end

function draw_player()
	spr(p.sprite,p.x*8+p.ox,p.y*8+p.oy,1,1,p.flip)
	if(p.item == 1) then
		spr(18,p.x*8+p.ox,p.y*8+p.oy,1,1,p.flip)
	end	
end

function pick_up_item(x,y)
	next_item(x,y)
	p.item+=1
	sfx(0)
end

function check_item(x,y)
	if check_flag(7,x,y) then
		pick_up_item(x,y)
	elseif check_flag(2,newx,newy) and p.item>0  then
		open_bridge(newx,newy)
	elseif check_flag(3,newx,newy) and p.keys > 0 then
		open_labyrinthe(newx,newy)
	end
end

function allow_player_movement()
	return current_dialog == nil or count(current_dialog) == 0
end
