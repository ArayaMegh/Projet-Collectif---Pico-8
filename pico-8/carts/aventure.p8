__lua__
function _init()
	create_player()
	damage={}
end

function _update60()
	show_dialog_if_needed()
	if allow_player_movement() then
		player_movement()
	end
	interact_with_dialog()
	update_camera()
end

function _draw()
	cls()
 draw_map()
 draw_player()
 if current_dialog and current_dialog[1] then
 	draw_dialog_box(2, 96, current_dialog[1].name, current_dialog[1].message)
 end
end
