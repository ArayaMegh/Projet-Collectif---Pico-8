--dialogues
current_dialog = {}


function show_dialog_if_needed()
  if p.item == 0 and p.x == 3 and p.y == 4 and p.ox == 0 and p.oy == 0 then
    current_dialog = dialog_1
  end
  if newx==11 and newy==5 then
    current_dialog = dialog_2
    p.keys = 1
  end
end

function interact_with_dialog()
		if btnp(❎) and count(current_dialog.messages) > 0 then 
  deli(current_dialog.messages, 1)
 end
	if current_dialog.id == 2 and count(current_dialog.messages) == 0 then
  p.max_hp += 2
 end
	if count(current_dialog.messages) == 0 then
 	current_dialog = {}
	end
end

